        "commitizen": "^4.1.2",
        "cz-conventional-changelog": "^3.2.0",
        "eslint": "^7.8.1",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-tsdoc": "^0.2.4",
        "husky": "^4.2.5",
        "lint-staged": "^10.2.2",
        "prettier": "^2.0.5",
        "semantic-release": "^17.0.7",
        "typescript": "^3.9.2"
